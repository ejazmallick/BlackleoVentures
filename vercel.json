{
"version": 2,
"buildCommand": "npm run build",
"outputDirectory": "dist/public",
"functions": {
"server/index.ts": {
"includeFiles": "node_modules/@google/genai/**",
"memory": 1024,
"maxDuration": 10
}
},
"routes": [
{
"src": "/api/(.)",
"dest": "server/index.ts"
},
{
"src": "/(.)",
"dest": "/index.html"
}
]
}